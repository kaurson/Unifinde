# Supabase Database Configuration
# Get these values from your Supabase project dashboard

# Database URL for Supabase
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres

# Example format:
# DATABASE_URL=postgresql://postgres:your_password@db.abcdefghijklmnop.supabase.co:5432/postgres

# Supabase API Keys (for future use with FastAPI)
SUPABASE_URL=https://[YOUR-PROJECT-REF].supabase.co
SUPABASE_ANON_KEY=[YOUR-ANON-KEY]
SUPABASE_SERVICE_ROLE_KEY=[YOUR-SERVICE-ROLE-KEY]

# Browser-use Configuration (Chromium-based web scraping)
SCRAPER_TYPE=browser_use
SCRAPER_HEADLESS=true
SCRAPER_DISABLE_SECURITY=true
CHROME_INSTANCE_PATH=
EXTRA_CHROMIUM_ARGS=

# LLM Configuration
LLM_PROVIDER=openai
LLM_API_KEY=your_openai_api_key_here
LLM_MODEL=gpt-4
LLM_BASE_URL=
LLM_TEMPERATURE=0.1
LLM_MAX_TOKENS=1000
LLM_TIMEOUT=30 